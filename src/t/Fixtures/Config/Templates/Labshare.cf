projectnumber: 	activity
group:          group
creator: 		creator
supervisor:     supervisor
datecreated:    collectiondate date (\d+)-(\d+)-(\d+) YEAR MON DAY
description:	description
service:		service
access:         access
share:          share
datastreams:    location